{
  "root": true,
  "extends": "plugin:prettier/recommended",
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "commonjs": false,
    "browser": false,
    "es6": true,
    "jest": true
  },
  "rules": {
    "prettier/prettier": "warn",
    "no-template-curly-in-string": 0,
    "quotes": [
      "error",
      "single",
      { "allowTemplateLiterals": true, "avoidEscape": true }
    ],
    "eqeqeq": 2,
    "no-unused-expressions": 1,
    "comma-dangle": 1,
    "no-cond-assign": 1,
    "no-console": 0,
    "no-constant-condition": 1,
    "no-control-regex": 1,
    "no-debugger": 1,
    "no-dupe-args": 1,
    "no-dupe-keys": 1,
    "no-duplicate-case": 1,
    "no-empty-character-class": 1,
    "no-empty": 1,
    "no-empty-pattern": 1,
    "no-ex-assign": 1,
    "no-extra-boolean-cast": 1,
    "no-extra-semi": 1,
    "no-func-assign": 1,
    "no-inner-declarations": 1,
    "no-invalid-regexp": 1,
    "no-irregular-whitespace": 1,
    "no-negated-in-lhs": 1,
    "no-obj-calls": 1,
    "no-regex-spaces": 1,
    "no-sparse-arrays": 1,
    "no-unreachable": 1,
    "use-isnan": 1,
    "valid-typeof": 1,
    "no-fallthrough": 1,
    "no-octal": 2,
    "no-redeclare": 2,
    "no-delete-var": 0,
    "no-undef": 2,
    "no-unused-vars": 1,
    "no-mixed-spaces-and-tabs": 2,
    "jsx-a11y/anchor-is-valid": 0,
    "array-callback-return": 2,
    "require-atomic-updates": 1
  }
}
